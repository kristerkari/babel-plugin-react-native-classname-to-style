{
  "name": "babel-plugin-react-native-classname-to-style",
  "version": "1.2.2",
  "description": "Transform JSX className property to style property in react-native.",
  "keywords": [
    "babel",
    "babel-plugin",
    "react-native"
  ],
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
  },
  "author": "Krister Kari",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kristerkari/babel-plugin-react-native-classname-to-style"
  },
  "files": [
    "CHANGELOG.md",
    "index.js",
    "README.md"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/eslint-plugin-development": "^1.0.1",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-tester": "^6.3.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.14.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "plugins": [
      "@babel/development"
    ],
    "env": {
      "jest": true
    },
    "rules": {
      "space-before-function-paren": [
        2,
        "never"
      ],
      "semi": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "always"
      ],
      "quotes": 0,
      "no-unused-vars": 2,
      "@babel/development/no-deprecated-clone": 2,
      "@babel/development/no-undefined-identifier": 2,
      "@babel/development/plugin-name": 2
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none"
  },
  "jest": {
    "coverageDirectory": "./.coverage/",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  }
}
